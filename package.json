{
    "name": "netcracker-graduation-work",
    "browserslist": [
        "> 5%"
    ],
    "scripts": {
        "ng": "ng",
        "lint-staged": "lint-staged --no-stash --config .lintstagedrc.json",
        "postinstall": "ngcc --properties es2015 browser module main --first-only",
        "start": "ng serve",
        "hmr": "ng serve --configuration hmr",
        "lint": "npm run eslint && stylelint --syntax less \"src/{stt-app,assets}/**/*.less\" --fix",
        "lint:prod": "npm run eslint:prod && stylelint \"./src/{stt-app,assets}/**/*.less\"",
        "eslint": "eslint \"./src/**/{*.ts,*.html}\" --fix",
        "eslint:prod": "eslint \"./src/**/{*.ts,*.html}\"",
        "prod": "npm run lint:prod && ng build --prod",
        "prepare": "husky install"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "13.1.3",
        "@angular/common": "13.1.3",
        "@angular/compiler": "13.1.3",
        "@angular/core": "13.1.3",
        "@angular/forms": "13.1.3",
        "@angular/platform-browser": "13.1.3",
        "@angular/platform-browser-dynamic": "13.1.3",
        "@angular/router": "13.1.3",
        "@ngrx/effects": "13.0.2",
        "@ngrx/entity": "13.0.2",
        "@ngrx/router-store": "13.0.2",
        "@ngrx/store": "13.0.2",
        "lodash.clonedeep": "4.5.0",
        "moment": "2.29.1",
        "rxjs": "7.5.2",
        "tslib": "2.3.1",
        "zone.js": "0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "13.1.4",
        "@angular-eslint/eslint-plugin": "13.0.1",
        "@angular-eslint/eslint-plugin-template": "13.0.1",
        "@angular-eslint/template-parser": "13.0.1",
        "@angular/cli": "13.1.4",
        "@angular/compiler-cli": "13.1.3",
        "@angular/language-service": "13.1.3",
        "@angularclass/hmr": "3.0.0",
        "@ngrx/store-devtools": "13.0.2",
        "@ngx-translate/core": "14.0.0",
        "@ngx-translate/http-loader": "7.0.0",
        "@types/lodash.clonedeep": "4.5.6",
        "@types/node": "14.14.31",
        "@typescript-eslint/eslint-plugin": "5.10.0",
        "@typescript-eslint/parser": "5.10.0",
        "eslint": "8.7.0",
        "eslint-plugin-etc": "2.0.2",
        "eslint-plugin-ngrx": "2.0.3",
        "eslint-plugin-rxjs": "5.0.1",
        "eslint-plugin-rxjs-angular": "2.0.0",
        "eslint-plugin-unicorn": "40.1.0",
        "gulp": "4.0.2",
        "husky": "7.0.4",
        "lint-staged": "12.3.1",
        "stylelint": "14.3.0",
        "ts-node": "10.4.0",
        "typescript": "4.4.4",
        "webpack-bundle-analyzer": "4.5.0",
        "postcss-less": "6.0.0"
    }
}
